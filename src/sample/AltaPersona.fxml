<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="140.0" prefWidth="221.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AltaPersonaController">
    <children>
        <Label fx:id="label" layoutX="14.0" layoutY="14.0" minHeight="16" minWidth="69" text="Alta de una persona" textAlignment="CENTER" textFill="#5f82ae">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font></Label>
        <Button fx:id="button" layoutX="35.0" layoutY="105.0" onAction="#aceptarPersona" text="Aceptar" />
        <TextField fx:id="campoNombre" layoutX="33.0" layoutY="70.0" />
      <Label layoutX="69.0" layoutY="47.0" text="Dar el nombre:" />
      <Button layoutX="130.0" layoutY="105.0" mnemonicParsing="false" onAction="#cancelarPersona" text="Cancelar" />
    </children>
</AnchorPane>
